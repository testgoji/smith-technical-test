var i = 1;

while (i < 101) {
	if ((i % 3 === 0) && (i % 5 !== 0)) {
	  console.log('Fizz');
	} else if ((i % 3 !== 0) && (i % 5 === 0)) {
	  console.log('Buzz');
	} else if ((i % 3 === 0) && (i % 5 === 0)) {
          console.log('FizzBuzz');
	} else {
	  console.log(i);
        };

	i = i + 1;
};
